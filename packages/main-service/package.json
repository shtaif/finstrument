{
  "name": "main-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "engines": {
    "bun": "^1.1.20"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently -r -P \"pnpm dev:app\" \"pnpm dev:gql-codegen-watch\"",
    "dev:app": "bun --watch ./src/index.ts",
    "dev:app1___": "ts-node-dev --inspect --env-file=.env -- ./src/index.ts",
    "dev:app2___": "tsx watch --clear-screen=false --env-file=.env ./src/index.ts",
    "dev:app3___": "NODE_OPTIONS=\"--env-file=.env\" ts-node-esm ./src/index.ts",
    "dev:gql-codegen-watch": "cd ../main-service-gql-codegen && pnpm start",
    "dev:gql-codegen-watch___": "graphql-codegen-esm --config ./gql-codegen-config.ts",
    "test": "NODE_COMPILE_CACHE=./_node_compile_cache vitest run --config ./spec/vitest.config.ts",
    "dev:test": "NODE_COMPILE_CACHE=./_node_compile_cache vitest --config ./spec/vitest.config.ts",
    "dev:test-with-gql-codegen": "concurrently -r -P \"cd ../main-service-gql-codegen && pnpm start\" \"pnpm dev:test {*}\"",
    "build": "rm -rf ./dist && tsc --outDir ./dist && copyfiles -u 1 ./src/**/*.graphql ./dist/",
    "build-and-deploy": "pnpm build && pnpm deploy --prod --filter=${npm_package_name} ./deployed"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@graphql-tools/schema": "^10.0.3",
    "@ngrok/ngrok": "^0.5.2",
    "@reactivex/ix-esnext-esm": "^5.0.0",
    "axios": "^1.6.8",
    "better-sse": "^0.9.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.4.0",
    "dataloader": "^2.2.2",
    "eventsource": "^2.0.2",
    "express": "^4.19.2",
    "graphql": "16.9.0",
    "graphql-ws": "^5.16.0",
    "http-terminator": "^3.2.0",
    "iter-tools": "^7.5.3",
    "iterable-operators": "workspace:^",
    "iterified": "^1.2.5",
    "ix": "^5.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "sequelize": "^6.35.2",
    "sequelize-typescript": "^2.1.6",
    "shared-utils": "workspace:^",
    "uuid": "^9.0.1",
    "ws": "^8.18.0",
    "yahoo-finance2": "^2.11.3",
    "znv": "^0.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@parcel/watcher": "^2.3.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/eventsource": "^1.1.11",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.14.11",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "utility-types": "^3.10.0",
    "vitest": "~2.0.2"
  }
}
